entity Customer{
	custId Integer,
	custName String,
    custEmail String,
    custAddress String
}

entity Order{
	orderId Integer,
    orderDate Instant    
}

entity OrderDetail{
	custID Integer,
    orderId Integer,
    itemID Integer,
    qty Integer,
    unitPrice Double
}

relationship OneToMany{
	OrderDetail{Customer(custId)} to Customer
}
    

relationship OneToMany{
 OrderDetail{Order(orderId)} to Order
}

dto * with mapstruct

angularSuffix * with mySuffix